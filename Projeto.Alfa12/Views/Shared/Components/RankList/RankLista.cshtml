@model IEnumerable<Aluno>
<table class="table is-bordered is-striped is-narrow is-hoverable is-fullwidth ">
    <thead>
        <tr>
    <th>Aluno</th>
    <th>Pontos</th>
    </tr>
    </thead>
    @foreach (var p in Model)
    {
        <tr class="has-text-centered">
            <td>@p.FullName</td>
            <td class="text-right">
                @p.PontoParcial
            </td>
        </tr>
    }
    
</table>